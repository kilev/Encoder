<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.AreaChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kil.MainWindowController">
   <children>
      <HBox alignment="CENTER">
         <children>
            <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="multiplicity A">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <TableView fx:id="multiplicityA" editable="true" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                     <columns>
                        <TableColumn fx:id="multiplicityAColumnAlvl" prefWidth="75.0" text="aLvl" />
                        <TableColumn fx:id="multiplicityAColumnLow" prefWidth="75.0" text="low" />
                        <TableColumn fx:id="multiplicityAColumnHigh" prefWidth="75.0" text="high" />
                     </columns>
                  </TableView>
                  <Label text="multiplicity B">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <TableView fx:id="multiplicityB" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                    <columns>
                      <TableColumn fx:id="multiplicityBColumnAlvl" prefWidth="75.0" text="aLvl" />
                      <TableColumn fx:id="multiplicityBColumnLow" prefWidth="75.0" text="low" />
                        <TableColumn fx:id="multiplicityBColumnHigh" prefWidth="75.0" text="hight" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </TableView>
                  <Label text="Compare A to B" />
                  <HBox alignment="CENTER">
                     <children>
                        <ToggleButton fx:id="lessIndicator" disable="true" mnemonicParsing="false" stylesheets="@MainWindow.css" text="&lt;" />
                        <ToggleButton fx:id="moreIndicator" disable="true" mnemonicParsing="false" stylesheets="@MainWindow.css" text="&gt;" />
                        <ToggleButton fx:id="equalIndicator" disable="true" mnemonicParsing="false" stylesheets="@MainWindow.css" text="=" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Button fx:id="plusButton" mnemonicParsing="false" onAction="#plusMultiplicity" text="+">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="minusButton" mnemonicParsing="false" onAction="#minusMultiplicity" text="-">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="multiButton" mnemonicParsing="false" onAction="#multiMultiplicity" text="*">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="divideButton" mnemonicParsing="false" onAction="#divideMultiplicity" text="/">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <Label text="multiplicity C">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <TableView fx:id="multiplicityC" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="multiplicityCColumnAlvl" prefWidth="75.0" text="aLvl" />
                      <TableColumn fx:id="multiplicityCColumnLow" prefWidth="75.0" text="low" />
                        <TableColumn fx:id="multiplicityCColumnHigh" prefWidth="75.0" text="hight" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </TableView>
               </children>
            </VBox>
            <VBox prefHeight="865.0" prefWidth="691.0">
               <children>
                  <AreaChart fx:id="chart" VBox.vgrow="ALWAYS">
                    <xAxis>
                      <NumberAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </AreaChart>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</Pane>
